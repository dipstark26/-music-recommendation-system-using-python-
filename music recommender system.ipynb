   ],
   "source": [
    "songs = pd.read_csv('spotify_millsongdata.csv')\n",
    "print(\"Dataset loaded successfully!\")\n",
    "print(songs.head())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "1f85d448-30df-4b3c-bf3d-f5519a634c13",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Missing values handled.\n"
     ]
    }
   ],
   "source": [
    "songs['text'] = songs['text'].fillna('')  \n",
    "print(\"Missing values handled.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "3c3ebdc5-0664-4955-994e-c215f2bd6255",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Text vectorized.\n"
     ]
    }
   ],
   "source": [
    "vectorizer = TfidfVectorizer(stop_words='english')  # Remove common stopwords\n",
    "text_vectors = vectorizer.fit_transform(songs['text'])\n",
    "print(\"Text vectorized.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "f8133de9-10d9-4e33-afed-7fb13496365c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Vectorizer saved as 'vectorizer.pkl'.\n"
     ]
    }
   ],
   "source": [
    "with open('vectorizer.pkl', 'wb') as vec_file:\n",
    "    pickle.dump(vectorizer, vec_file)\n",
    "print(\"Vectorizer saved as 'vectorizer.pkl'.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "2cc9e8e4-749c-4cfa-87a3-cb6c85cfc8b7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Dimensionality reduced.\n"
     ]
    }
   ],
   "source": [
    "svd = TruncatedSVD(n_components=100)  # Reduce to 100 dimensions\n",
    "text_vectors_reduced = svd.fit_transform(text_vectors)\n",
    "print(\"Dimensionality reduced.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "2ba4ce62-ac07-4782-9903-30fef9e2d06f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Reduced vectors saved as 'reduced_vectors.pkl'.\n"
     ]
    }
   ],
   "source": [
    "with open('reduced_vectors.pkl', 'wb') as vec_file:\n",
    "    pickle.dump(text_vectors_reduced, vec_file)\n",
    "print(\"Reduced vectors saved as 'reduced_vectors.pkl'.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "5e13dbc1-c22b-457c-9aaf-bf3925d2e06d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Faiss index created.\n"
     ]
    }
   ],
   "source": [
    "text_vectors_dense = text_vectors_reduced.astype('float32')\n",
    "faiss.normalize_L2(text_vectors_dense)  \n",
    "index = faiss.IndexFlatIP(text_vectors_dense.shape[1])  \n",
    "index.add(text_vectors_dense)  \n",
    "print(\"Faiss index created.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "fa01797a-111c-441a-9cad-ec5d84dad202",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "FAISS index saved as 'faiss_index.bin'.\n"
     ]
    }
   ],
   "source": [
    "faiss.write_index(index, 'faiss_index.bin')\n",
    "print(\"FAISS index saved as 'faiss_index.bin'.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "48a4cf44-56f3-4a1c-bab2-ca5616418ef0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def recommend_songs_iterative(song_index, top_n=5):\n",
    "    query_vector = text_vectors_reduced[song_index].reshape(1, -1)\n",
    "    similarities = cosine_similarity(query_vector, text_vectors_reduced).flatten()\n",
    "    similar_indices = similarities.argsort()[-top_n-1:-1][::-1]\n",
    "    return songs.iloc[similar_indices]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "e4ce7899-50a5-4b3f-990e-985a107cd559",
   "metadata": {},
   "outputs": [],
   "source": [
    "def recommend_songs_faiss(song_index, top_n=5):\n",
    "    query_vector = text_vectors_dense[song_index].reshape(1, -1)\n",
    "    faiss.normalize_L2(query_vector)  # Normalize the query vector\n",
    "    _, indices = index.search(query_vector, top_n + 1)  # Perform search\n",
    "    return songs.iloc[indices[0][1:]]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "ac18c879-ac00-4d5f-87d4-0791725dcb17",
   "metadata": {},
   "outputs": [],
   "source": [
    "def main():\n",
    "    st.title(\"Music Recommender System ðŸŽ¶\")\n",
